{
  "expo": {
    "name": "Seat Track",
    "slug": "seat-track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/seat-track-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.nava.seatTrack",
      "infoPlist": {
        "CFBundleDisplayName": "Seat Track"
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/seat-track-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#D06425"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
